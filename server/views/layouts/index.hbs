<!DOCTYPE html>
<html lang="{{locale}}">
  <head>
    <meta charset="utf-8">
    <link rel="icon" type="image/ico" href="/favicon.ico">
    {{#each assets.style}}
      <link rel="stylesheet" href="{{this}}">
    {{/each}}
    <title>{{{title}}}</title>
  </head>
  <body>
    {{{body}}}
    {{#each assets.script}}
      <script src="{{this}}"></script>
    {{/each}}
    <script async defer id="github-bjs" src="https://buttons.github.io/buttons.js"></script>
  </body>
</html>
